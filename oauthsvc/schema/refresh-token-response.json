{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://localhost:3000/schemas/refresh-token-response.json#",
  "title": "refresh",
  "type": "object",
  "additionalProperties": false,
  "required": [ "token_type", "access_token", "expires_in" ],
  "properties": {
    "token_type": {
    	"type": "string",
      "enum": ["bearer"]
	  },
    "access_token": { 
    	"type": "string"
    },
    "expires_in": {
      "type": "number"
    },
    "refresh_token": {
      "type": "string"
    }
  }
}
