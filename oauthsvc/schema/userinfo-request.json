{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://localhost:3000/schemas/userinfo-request.json#",
  "title": "userinfo",
  "resources": "GET /userinfo",
  "type": "object",
  "additionalProperties": false,
  "required": [ "code", "grant_type"],
  "properties": {
    "Authorization": {
    	"type": "string",
      "description": "The bearer access token, scoped to retrieve the consented claims for the subject (end-user)."
	  }
  }
}
 
